<launch>

<rosparam file="$(find urdf-model_description)/launch/controller.yaml" command="load"/>
<node args="Rev7_position_controller Rev8_position_controller joint_state_controller " name="controller_spawner" ns="urdf-model" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/urdf-model/joint_states"/>
</node>

</launch>